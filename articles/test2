<td><i>g</i><sub>−1</sub>
</td>
<td style="background: #eef">γ<sub>1</sub>
</td>
<td><i>g</i><sub>0</sub>
</td>
<td style="background: #eef">γ<sub>2</sub>
</td>
<td><i>g</i><sub>1</sub>
</td>
<td style="background: #eef">γ<sub>3</sub>
</td>
<td><i>g</i><sub>2</sub>
</td>
<td style="background: #eef">γ<sub>4</sub>
</td>
<td><i>g</i><sub>3</sub>
</td>
<td style="background: #eef">γ<sub>5</sub>
</td>
<td><i>g</i><sub>4</sub>
</td>
