              <mi>x</mi>
              <msup>
                <mo stretchy="false">)</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mi>k</mi>
                </mrow>
              </msup>
            </mrow>
            <mrow>
              <mo stretchy="false">(</mo>
              <mi>k</mi>
              <mo>−<!-- − --></mo>
              <mn>1</mn>
