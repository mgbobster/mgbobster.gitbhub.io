            </mrow>
          </mfrac>
        </mrow>
        <mi>log</mi>
        <mo>⁡<!-- ⁡ --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mi>T</mi>
            <mrow>
              <mn>2</mn>
              <mi>π<!-- π --></mi>
              <mi>e</mi>
            </mrow>
          </mfrac>
        </mrow>
