              <mo>!</mo>
              <mi>ζ<!-- ζ --></mi>
              <mo stretchy="false">(</mo>
              <mn>2</mn>
              <mi>k</mi>
              <mo stretchy="false">)</mo>
            </mrow>
          </mfrac>
        </mrow>
        <mo>=</mo>
        <mi>O</mi>
        <mrow>
          <mo>(</mo>
          <msup>
            <mi>x</mi>
            <mrow class="MJX-TeXAtom-ORD">
